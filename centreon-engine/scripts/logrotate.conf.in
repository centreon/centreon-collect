@VAR_DIR@/centengine.log {
  compress
  copytruncate
  create 640 @USER@ @GROUP@
  daily
  delaycompress
  missingok
  olddir @LOG_ARCHIVE_DIR@
  rotate 30
}

@VAR_DIR@/centengine.debug {
  compress
  copytruncate
  create 640 @USER@ @GROUP@
  delaycompress
  missingok
  olddir @LOG_ARCHIVE_DIR@
  rotate 5
  size   1G
}
