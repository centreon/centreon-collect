version: '2'
services:
  web:
    image: @WEB_IMAGE@
    ports:
    - "80"
    - "3306"
    links:
    - influxdb
    external_links:
    - webdriver
    networks:
    - default
    - webdriver_default
  influxdb:
   image: influxdb:latest
   ports:
   - "8083"
   - "8086"
networks:
  webdriver_default:
    external: true