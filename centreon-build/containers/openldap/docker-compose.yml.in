version: '2'
services:
  webdriver:
    image: selenium/standalone-chrome:latest
    ports:
    - "4444"
    environment:
    - DBUS_SESSION_BUS_ADDRESS=/dev/null
    volumes:
    - /dev/shm:/dev/shm
  web:
    image: @WEB_IMAGE@
    ports:
    - "80"
    - "3306"
    links:
    - openldap
  openldap:
    image: ci.int.centreon.com:5000/mon-openldap:latest
    ports:
    - "389"
