version: '2'
services:
  web:
    image: @WEB_IMAGE@
    ports:
    - "80"
    - "3306"
    links:
    - openldap
    external_links:
    - webdriver
    networks:
    - default
    - webdriver_default
  openldap:
    image: registry.centreon.com/mon-openldap:latest
    ports:
    - "389"
    external_links:
    - webdriver
    networks:
    - default
    - webdriver_default
networks:
  webdriver_default:
    external: true