version: '2'
services:
  web:
    image: @WEB_IMAGE@
    ports:
    - "80"
    - "3306"
    dns:
    - "10.50.1.1"
    links:
    - squid
    external_links:
    - webdriver
    networks:
    - default
    - webdriver_default
  squid:
    image: ci.int.centreon.com:5000/mon-squid-simple:latest
    ports:
    - "3128"
    external_links:
    - webdriver
    networks:
    - default
    - webdriver_default
networks:
  webdriver_default:
    external: true