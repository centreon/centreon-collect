# Sources.
COPY api-web-import-export/api-web-import-export.conf.php /usr/share/centreon/www/modules/api-web-import-export/api-web-import-export.conf.php
COPY api-web-import-export/core /usr/share/centreon/www/modules/api-web-import-export/core
