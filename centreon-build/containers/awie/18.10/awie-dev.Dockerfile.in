# Base information.
FROM ci.int.centreon.com:5000/mon-awie-18.10:@DISTRIB@
LABEL maintainer="Matthieu Kermagoret <mkermagoret@centreon.com>"

# Database.
COPY awie/18.10/install-dev.sh /tmp/install.sh
COPY centreon-awie/sql /usr/share/centreon/www/modules/centreon-awie/sql
RUN /tmp/install.sh

# Sources.
COPY centreon-awie/centreon-awie.conf.php /usr/share/centreon/www/modules/centreon-awie/centreon-awie.conf.php
COPY centreon-awie/core /usr/share/centreon/www/modules/centreon-awie/core
