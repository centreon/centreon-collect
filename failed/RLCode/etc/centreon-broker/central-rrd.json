    "log": {
      "log_pid": "yes",
      "log_source": "no",
      "flush_period": 0,
      "directory": "/tmp/var/log/centreon-broker/",
      "filename": "",
      "max_size": 0,
      "loggers": {
        "core": "trace",
        "config": "error",
        "sql": "error",
        "processing": "error",
        "perfdata": "error",
        "bbdo": "info",
        "tcp": "info",
        "tls": "trace",
        "lua": "error",
        "bam": "error"
      }
