#ifndef CCC_ICMP_VERSION_HH
# define CCC_ICMP_VERSION_HH

// Compile-time values.
# define CENTREON_CONNECTOR_ICMP_VERSION_MAJOR  @CONNECTOR_ICMP_MAJOR@
# define CENTREON_CONNECTOR_ICMP_VERSION_MINOR  @CONNECTOR_ICMP_MINOR@
# define CENTREON_CONNECTOR_ICMP_VERSION_PATCH  @CONNECTOR_ICMP_PATCH@
# define CENTREON_CONNECTOR_ICMP_VERSION_STRING "@CONNECTOR_ICMP_VERSION@"

# define CENTREON_ENGINE_VERSION_MAJOR  @CONNECTOR_ICMP_MAJOR@
# define CENTREON_ENGINE_VERSION_MINOR  @CONNECTOR_ICMP_MINOR@

# include "com/centreon/connector/icmp/namespace.hh"

CCC_ICMP_BEGIN()

namespace            version {
  // Compile-time values.
  unsigned int const major = @CONNECTOR_ICMP_MAJOR@;
  unsigned int const minor = @CONNECTOR_ICMP_MINOR@;
  unsigned int const patch = @CONNECTOR_ICMP_PATCH@;

  unsigned int const engine_major = @CONNECTOR_ICMP_MAJOR@;
  unsigned int const engine_minor = @CONNECTOR_ICMP_MINOR@;

  // Run-time values.
  unsigned int       get_major() throw ();
  unsigned int       get_minor() throw ();
  unsigned int       get_patch() throw ();
  char const*        get_string() throw ();

  unsigned int       get_engine_major() throw ();
  unsigned int       get_engine_minor() throw ();
}

CCC_ICMP_END()

#endif // !CCC_ICMP_VERSION_HH
