<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>My index</title>
        <!-- inject:css -->
        <link rel="stylesheet" href="/{$webPath}/lib/css/main.css">
        <link rel="stylesheet" href="/{$webPath}/lib/css/normalize.css">
        <!-- endinject -->
        <script>
            var $Automation = jQuery.noConflict(true);
        </script>
    </head>
    <body>

        {if $genericActiveHostAvailable}
        <div id="main" class="global-container">
            <div class="side-container"></div>
            <div class="view-container"></div>
        </div>
        {else}
        <div class='msg' align='center'>
            {t}"generic-active-host" template not found{/t}
        </div>
        {/if}
        <!-- inject:js -->
        <script src="/{$webPath}/lib/js/libs.js"></script>
        <script src="/{$webPath}/lib/js/app.js"></script>
        <script src="/{$webPath}/lib/js/main.js"></script>
        <script src="/{$webPath}/lib/js/templates.js"></script>
        <!-- endinject -->
        
    </body>

</html>