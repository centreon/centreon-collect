version: '2'
services:
  web:
    image: @WEB_IMAGE@
    ports:
    - "80"
    - "3306"
    links:
    - influxdb
  influxdb:
   image: influxdb:latest
   ports:
   - "8083"
   - "8086"
  webdriver:
    image: selenium/standalone-chrome-debug:latest
    environment:
    - "SCREEN_WIDTH=1600"
    - "SCREEN_HEIGHT=2000"
    ports:
    - "4444"
    - "5900"
    volumes:
    - /dev/shm:/dev/shm
