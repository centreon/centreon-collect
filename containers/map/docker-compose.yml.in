version: '2'
services:
  web:
    image: @WEB_IMAGE@
    ports:
    - "80"
    - "3306"
    dns:
    - "10.50.1.1"
  map:
    image: @MAP_IMAGE@
    ports:
    - "8080"
    - "8443"
    dns:
    - "10.50.1.1"
  webdriver:
    image: selenium/standalone-chrome-debug:3.141.59-oxygen
    environment:
    - "SCREEN_WIDTH=1600"
    - "SCREEN_HEIGHT=2000"
    ports:
    - "4444"
    - "5900"
    volumes:
    - /dev/shm:/dev/shm
