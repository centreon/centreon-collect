version: '2'
services:
  webdriver:
    image: selenium/standalone-chrome:latest
    ports:
    - "4444"
    environment:
    - DBUS_SESSION_BUS_ADDRESS=/dev/null
    volumes:
    - /dev/shm:/dev/shm
  web:
    image: @WEB_IMAGE@
    ports:
    - "80"
    - "3306"
    dns:
    - "10.50.1.1"
    volumes:
    - "@SOURCE_DIR@/module:/usr/share/centreon/www/modules/centreon-map4-web-client"
    - "@SOURCE_DIR@/widget:/usr/share/centreon/www/widgets/centreon-map4-web-client"
  map:
    image: @MAP_IMAGE@
    ports:
    - "8080"
    - "8443"
    dns:
    - "10.50.1.1"
