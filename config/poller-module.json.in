{
    "centreonBroker": {
        "broker_id": 3,
        "broker_name": "central-module",
        "poller_id": 1,
        "poller_name": "Central",

        "module_directory": "@CMAKE_INSTALL_PREFIX@/@CMAKE_INSTALL_DATAROOTDIR@/centreon/lib/centreon-broker",
        "log_timestamp": false,
        "log_thread_id": false,
        "event_queue_max_size": 100000,
        "command_file": "@CMAKE_INSTALL_PREFIX@/@CMAKE_INSTALL_LOCALSTATEDIR@/lib/centreon-broker/poller-module.cmd",
        "cache_directory": "@CMAKE_INSTALL_PREFIX@/@CMAKE_INSTALL_LOCALSTATEDIR@/lib/centreon-broker",
        "logger": [
            {
                "name": "@CMAKE_INSTALL_PREFIX@/@CMAKE_INSTALL_LOCALSTATEDIR@/log/centreon-broker/poller-module.log",
                "config": "yes",
                "debug": "no",
                "error": "yes",
                "info": "no",
                "level": "low",
                "type": "file"
            }
        ],
        "output": [
            {
                "name": "central-module-output",
                "port": "5669",
                "host": "localhost",
                "retry_interval": "60",
                "buffering_timeout": "0",
                "protocol": "bbdo",
                "tls": "no",
                "negotiation": "yes",
                "one_peer_retention_mode": "no",
                "compression": "no",
                "type": "ipv4"
            }
        ],
        "stats": [
            {
                "type": "stats",
                "name": "poller-module-stats",
                "json_fifo": "@CMAKE_INSTALL_PREFIX@/@CMAKE_INSTALL_LOCALSTATEDIR@/lib/centreon-broker/poller-module-stats.json"
            }
        ]
    }
}