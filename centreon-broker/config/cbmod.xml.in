<?xml version="1.0" encoding="UTF-8"?>
<conf>
 <module_directory>@PREFIX_MODULES@</module_directory>
 <instance>1</instance>
 <instance_name>broker-module</instance_name>
 <event_queue_max_size>50000</event_queue_max_size>
 <flush_logs>no</flush_logs>
 <log_thread_id>no</log_thread_id>
 <log_timestamp>yes</log_timestamp>
 <name>broker-module</name>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <type>ipv4</type>
  <name>central-output</name>
  <port>5668</port>
  <host>localhost</host>
  <failover>failover-central-output</failover>
  <protocol>bbdo</protocol>
  <filters>
   <category>neb</category>
  </filters>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <max_size>200000000</max_size>
  <protocol>bbdo</protocol>
  <type>file</type>
  <name>failover-central-output</name>
  <path>/var/lib/centreon-broker/central-output.failover</path>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <type>dumper</type>
  <tagname>extcommand-Central</tagname>
  <path>/var/lib/centreon-engine/rw/centengine.cmd</path>
  <name>extcommand-Central</name>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <type>dumper</type>
  <tagname>cfg-engine-Central</tagname>
  <path>/etc/centreon-engine/$FILENAME$</path>
  <name>cfg-engine-Central</name>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <type>dumper</type>
  <tagname>cfg-broker-Central</tagname>
  <path>/etc/centreon-broker/$FILENAME$</path>
  <name>cfg-broker-Central</name>
 </output>
 <input>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <one_peer_retention_mode>no</one_peer_retention_mode>
  <protocol>bbdo</protocol>
  <negociation>yes</negociation>
  <type>tcp</type>
  <name>central-transfert-input</name>
  <host>localhost</host>
  <port>5669</port>
 </input>
 <logger>
  <config>no</config>
  <debug>no</debug>
  <error>yes</error>
  <info>no</info>
  <level>2</level>
  <type>file</type>
  <name>/var/log/centreon-broker/broker-module.log</name>
 </logger>
 <stats>
  <type>stats</type>
  <name>stats-central-broker</name>
  <fifo>/var/lib/centreon-broker/stats-broker-module.fifo</fifo>
 </stats>
</conf>
