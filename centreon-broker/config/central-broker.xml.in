<?xml version="1.0" encoding="UTF-8"?>
<conf>
 <module_directory>@PREFIX_MODULE@</module_directory>
 <instance>1</instance>
 <instance_name>Central</instance_name>
 <event_queue_max_size>50000</event_queue_max_size>
 <flush_logs>no</flush_logs>
 <log_thread_id>no</log_thread_id>
 <log_timestamp>yes</log_timestamp>
 <name>central-broker</name>
 <input>
  <name>central-broker-external-command-input</name>
  <type>dump_fifo</type>
  <path>/var/lib/centreon-broker/extcommand-1.fifo</path>
  <tagname>central-broker-external-command</tagname>
 </input>
 <input>
  <name>central-engine-external-command-input</name>
  <type>dump_fifo</type>
  <path>/var/lib/centreon-broker/extcommand-2.fifo</path>
  <tagname>central-engine-external-command</tagname>
 </input>
 <input>
  <name>central-cfg-engine</name>
  <type>dump_dir</type>
  <path>/tmp/engine/apply/1</path>
  <tagname>cfg-engine-central</tagname>
 </input>
 <input>
  <name>central-cfg-broker</name>
  <type>dump_dir</type>
  <path>/tmp/broker/apply/1</path>
  <tagname>cfg-broker-central</tagname>
 </input>
 <input>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <one_peer_retention_mode>no</one_peer_retention_mode>
  <protocol>bbdo</protocol>
  <negociation>yes</negociation>
  <type>tcp</type>
  <name>central-input</name>
  <port>5668</port>
 </input>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>2</read_timeout>
  <retry_interval>60</retry_interval>
  <db_type>mysql</db_type>
  <db_host>localhost</db_host>
  <db_port>3306</db_port>
  <db_name>centreon</db_name>
  <db_user>centreon</db_user>
  <db_password>centreon</db_password>
  <with_state_events>yes</with_state_events>
  <queries_per_transaction>1000</queries_per_transaction>
  <check_replication>0</check_replication>
  <interval></interval>
  <length>15552000</length>
  <type>sql</type>
  <name>central-sql</name>
  <failover>failover-central-sql</failover>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <max_size>200000000</max_size>
  <protocol>bbdo</protocol>
  <type>file</type>
  <name>failover-central-sql</name>
  <path>/var/lib/centreon-broker/central-sql.failover</path>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>2</read_timeout>
  <retry_interval>60</retry_interval>
  <db_type>mysql</db_type>
  <db_host>localhost</db_host>
  <db_port>3306</db_port>
  <db_name>centreon</db_name>
  <db_user>centreon</db_user>
  <db_password>centreon</db_password>
  <queries_per_transaction>1000</queries_per_transaction>
  <check_replication>0</check_replication>
  <store_in_data_bin>yes</store_in_data_bin>
  <insert_in_index_data>yes</insert_in_index_data>
  <interval>60</interval>
  <length>15552000</length>
  <type>storage</type>
  <name>central-storage</name>
  <failover>failover-central-storage</failover>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <max_size>200000000</max_size>
  <protocol>bbdo</protocol>
  <type>file</type>
  <name>failover-central-storage</name>
  <path>/var/lib/centreon-broker/central-sql.failover</path>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <type>ipv4</type>
  <name>central-rrd</name>
  <port>5670</port>
  <host>localhost</host>
  <protocol>bbdo</protocol>
  <tls>no</tls>
  <negociation>yes</negociation>
  <compression>no</compression>
  <failover>failover-central-rrd</failover>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <max_size>200000000</max_size>
  <protocol>bbdo</protocol>
  <type>file</type>
  <name>failover-central-rrd</name>
  <path>/var/lib/centreon-broker/central-rrd.failover</path>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <one_peer_retention_mode>no</one_peer_retention_mode>
  <protocol>bbdo</protocol>
  <negociation>yes</negociation>
  <type>tcp</type>
  <name>central-transfert-output</name>
  <port>5669</port>
  <filters>
   <category>dumper</category>
  </filters>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <name>correlation</name>
  <type>correlation</type>
  <file>/etc/centreon-broker//correlation.xml</file>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <db_type>mysql</db_type>
  <db_host>localhost</db_host>
  <db_port>3306</db_port>
  <db_name>centreon</db_name>
  <db_user>centreon</db_user>
  <db_password>centreon</db_password>
  <type>bam</type>
  <name>centreon-bam-monitoring</name>
  <failover>failover-bam-monitoring</failover>
  <queries_per_transaction>0</queries_per_transaction>
  <command_file>/var/lib/centreon-engine/rw/centengine.cmd</command_file>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <max_size>500000000</max_size>
  <protocol>bbdo</protocol>
  <type>file</type>
  <name>failover-bam-monitoring</name>
  <path>/var/lib/centreon-broker/centreon-bam-monitoring.failover</path>
  <compression>no</compression>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <db_type>mysql</db_type>
  <db_host>localhost</db_host>
  <db_port>3306</db_port>
  <db_name>centreon</db_name>
  <db_user>centreon</db_user>
  <db_password>centreon</db_password>
  <type>bam_bi</type>
  <name>centreon-bam-reporting</name>
  <failover>failover-bam-reporting</failover>
  <queries_per_transaction>0</queries_per_transaction>
  <filters>
   <category>bam</category>
  </filters>
 </output>
 <output>
  <buffering_timeout>10</buffering_timeout>
  <read_timeout>10</read_timeout>
  <retry_interval>60</retry_interval>
  <max_size>500000000</max_size>
  <protocol>bbdo</protocol>
  <type>file</type>
  <name>failover-bam-reporting</name>
  <path>/var/lib/centreon-broker/centreon-bam-reporting.failover</path>
  <compression>no</compression>
 </output>
 <logger>
  <config>no</config>
  <debug>no</debug>
  <error>yes</error>
  <info>no</info>
  <level>2</level>
  <type>file</type>
  <name>/var/log/centreon-broker/central-broker.log</name>
 </logger>
 <stats>
  <type>stats</type>
  <name>stats-central-broker</name>
  <fifo>/var/lib/centreon-broker/stats-central-broker.fifo</fifo>
 </stats>
</conf>
