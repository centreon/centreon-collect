<?xml version="1.0" encoding="UTF-8" ?>
<centreonbroker>
  <include>@PROJECT_SOURCE_DIR@/test/cfg/broker_modules.xml</include>
  <instance>43</instance>
  <instance_name>my_cbd</instance_name>
  <cache_directory>@CACHE_DIRECTORY@</cache_directory>
  <event_queue_max_size>100</event_queue_max_size>
  <!--
  <stats>
    <fifo>/tmp/cbd.fifo</fifo>
  </stats>
  <logger>
    <type>file</type>
    <name>cbd.log</name>
    <config>1</config>
    <debug>1</debug>
    <error>1</error>
    <info>1</info>
    <level>3</level>
  </logger>
  -->
  <input>
    <name>AutomaticRetentionInput</name>
    <type>tcp</type>
    <port>5683</port>
    <protocol>bbdo</protocol>
  </input>
  <output>
    <name>AutomaticRetentionOutput</name>
    <type>sql</type>
    <db_type>@DB_TYPE@</db_type>
    <db_host>@DB_HOST@</db_host>
    <db_port>@DB_PORT@</db_port>
    <db_user>@DB_USER@</db_user>
    <db_password>@DB_PASSWORD@</db_password>
    <db_name>@DB_NAME@</db_name>
    <queries_per_transaction>0</queries_per_transaction>
  </output>
</centreonbroker>
