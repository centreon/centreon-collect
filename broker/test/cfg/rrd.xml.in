<?xml version="1.0" encoding="UTF-8" ?>
<centreonbroker>
  <include>@PROJECT_SOURCE_DIR@/test/cfg/broker_modules.xml</include>
  <broker_id>@BROKER_ID@</broker_id>
  <broker_name>@BROKER_NAME@</broker_name>
  <poller_id>@POLLER_ID@</poller_id>
  <poller_name>@POLLER_NAME@</poller_name>
  <!--
  <stats>
    <fifo>/tmp/rrd.fifo</fifo>
  </stats>
  <logger>
    <type>file</type>
    <name>rrd.log</name>
    <config>1</config>
    <debug>1</debug>
    <error>1</error>
    <info>1</info>
    <level>3</level>
  </logger>
  -->
  <input>
    <name>tcp</name>
    <type>tcp</type>
    <protocol>bbdo</protocol>
    <port>@TCP_PORT@</port>
  </input>
  <output>
    <name>storage</name>
    <type>storage</type>
    <db_type>@DB_TYPE@</db_type>
    <db_host>@DB_HOST@</db_host>
    <db_port>@DB_PORT@</db_port>
    <db_user>@DB_USER@</db_user>
    <db_password>@DB_PASSWORD@</db_password>
    <db_name>@DB_NAME@</db_name>
    <length>604800</length>
    <interval>@MONITORING_INTERVAL_LENGTH@</interval>
    <queries_per_transaction>50000</queries_per_transaction>
    @STORAGE_ADDITIONAL@
  </output>
  <output>
    <name>rrd</name>
    <type>rrd</type>
    <metrics_path>@METRICS_PATH@</metrics_path>
    <status_path>@STATUS_PATH@</status_path>
    @RRD_ADDITIONAL@
  </output>
</centreonbroker>
