  <stats>
    <fifo>/tmp/cbmod.fifo</fifo>
  </stats>
  <log_timestamp>1</log_timestamp>
  <logger>
    <type>file</type>
    <name>cbmod.log</name>
    <config>1</config>
    <debug>1</debug>
