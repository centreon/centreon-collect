  <output>
    <name><![CDATA[central-broker-master-perfdata]]></name>
    <length>1</length>
    <db_type>@DB_TYPE@</db_type>
    <db_host>@DB_HOST@</db_host>
    <db_port>@DB_PORT@</db_port>
    <db_user>@DB_USER@</db_user>
    <db_password>@DB_PASSWORD@</db_password>
    <db_name>@MYSQL_DB_NAME@</db_name>
    <insert_in_index_data>true</insert_in_index_data>
    <check_replication>no</check_replication>
    <type>storage</type>
  </output>
  <output>
    <name>GraphiteUnitTest</name>
    <type>graphite</type>
    <db_host>@GRAPHITE_DB_HOST@</db_host>
    <db_port>@GRAPHITE_DB_PORT@</db_port>
    <db_user>@GRAPHITE_DB_USER@</db_user>
    <db_password>@GRAPHITE_DB_PASSWORD@</db_password>
    <queries_per_transaction>2</queries_per_transaction>
    <metric_naming>centreon.$INSTANCE$.$METRIC$.$HOSTID$.$HOST$.$SERVICEID$.$SERVICE$.$INDEXID$.$METRICID$</metric_naming>
    <status_naming>centreon.$INSTANCE$.status.$HOSTID$.$HOST$.$SERVICEID$.$SERVICE$.$INDEXID$</status_naming>
  </output>
